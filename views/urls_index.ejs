<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <%- include('partials/_header') %>
    <table style='width: 50%'>
      <tr>
        <th>Full URL</th>
        <th>Shortened URL</th>
        <th>Options</th>
      </tr>
      <% for(let key in urls) { %>

        <tr>
          <td><%= urls[key] %></td>
          <td><%= key %><td>
          <td>
            <form method="POST" action='/urls/<%=key%>/delete'><input type='submit' value='Delete'></form>
          </td>
        </tr>
      <% } %>
    </table>
  <%- include('partials/_footer') %>
</body>
</html>